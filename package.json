    "redux": "^4.2.0",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.4.1",
    "regenerator-runtime": "0.13.7",
    "serve-favicon": "^2.5.0",
    "socket.io": "^4.5.0",
    "socket.io-client": "^4.5.0",
    "stream": "^0.0.2",
    "terser-webpack-plugin": "^5.3.0",
    "transform-class-properties": "1.0.0-beta",
