    "connected-react-router": "^6.9.2",
    "cookie-parser": "^1.4.6",
    "copy-webpack-plugin": "^10.2.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "css-minimizer-webpack-plugin": "^3.3.1",
    "dotenv": "^16.0.0",
    "eslint-webpack-plugin": "^3.1.1",
    "express": "^4.17.3",
    "history": "^5.3.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "mongoose": "^6.3.1",
    "path": "^0.12.7",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-helmet": "^6.1.0",
    "react-redux": "^8.0.1",
    "react-router": "^6.3.0",
